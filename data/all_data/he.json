    "Registers": {
        "wells": [
            "Registers are being stored in ~/.viminfo, and will be loaded again on next restart of vim.",
            "Special registers:\n          \n             0 - last yank\n             \" - unnamed register, last delete or yank\n             % - current file name\n             # - alternate file name\n             * - clipboard contents (X11 primary)\n             + - clipboard contents (X11 clipboard)\n             / - last search pattern\n             : - last command-line\n             . - last inserted text\n             - - last small (less than a line) delete\n             = - expression register\n             _ - black hole register"
        ],
        "item_1": {
            "keys": ":reg[isters]",
            "description": "show registers content"
        },
        "item_2": {
            "keys": "\"xy",
            "description": "yank into register x"
        },
        "item_3": {
            "keys": "\"xp",
            "description": "paste contents of register x"
